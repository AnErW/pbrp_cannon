# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

# Currently we dont have e2fsck compiled. So fs check would failed.

# <mount_point>  <fstype>  <device>        <device2>                  <flags>
/cache            ext4     /dev/block/platform/bootdevice/by-name/cache        flags=backup=1;wipeingui;wipeduringfactoryreset
/system_root      ext4     /dev/block/mapper/system
/super            emmc     /dev/block/platform/bootdevice/by-name/super        flags=display="super";flashimg=1
/boot             emmc     /dev/block/platform/bootdevice/by-name/boot         flags=backup=1;flashimg=1
/data             f2fs     /dev/block/platform/bootdevice/by-name/userdata     flags=fileencryption=aes-256-xts:aes-256-cts:v1wipeingui;wipeduringfactoryreset
/dtbo             emmc     /dev/block/platform/bootdevice/by-name/dtbo         flags=display="dtbo";flashimg=1
/recovery         emmc     /dev/block/platform/bootdevice/by-name/recovery     flags=backup=1;flashimg=1
/bootloader       emmc     /dev/block/platform/bootdevice/by-name/lk           flags=backup=1
/bootloader2      emmc     /dev/block/platform/bootdevice/by-name/lk2          flags=backup=1
/cust             ext4     /dev/block/platform/bootdevice/by-name/cust         flags=backup=1
/metadata         ext4     /dev/block/platform/bootdevice/by-name/md_udc       flags=backup=1
/protect_f        ext4     /dev/block/platform/bootdevice/by-name/protect1     flags=backup=1
/protect_s        ext4     /dev/block/platform/bootdevice/by-name/protect2     flags=backup=1
/nvdata           ext4     /dev/block/platform/bootdevice/by-name/nvdata       flags=backup=1
/nvcfg            ext4     /dev/block/platform/bootdevice/by-name/nvcfg        flags=backup=1
/persist          ext4     /dev/block/platform/bootdevice/by-name/persist      flags=display="persist";
/nvram            emmc     /dev/block/platform/bootdevice/by-name/nvram        flags=backup=1;flashimg=1
/proinfo          emmc     /dev/block/platform/bootdevice/by-name/proinfo      flags=backup=1
/logo             emmc     /dev/block/platform/bootdevice/by-name/logo         flags=backup=1
/tee1             emmc     /dev/block/platform/bootdevice/by-name/tee1         flags=backup=1
/tee2             emmc     /dev/block/platform/bootdevice/by-name/tee2         flags=display=backup=1
/vbmeta           emmc     /dev/block/platform/bootdevice/by-name/vbmeta       flags=display="vbmeta";backup=1;flashimg=1



/persistent       emmc     /dev/block/platform/bootdevice/by-name/frp

/external_sd      vfat     /dev/block/mmcblk0p1  /dev/block/mmcblk0            flags=display="external_sd";storage;wipeingui;removable;fsflags="utf8"
/usb-otg          vfat     /dev/block/sdd1  /dev/block/sdd                     flags=display="usb-otg";storage;wipeingui;removable;fsflags="utf8"

